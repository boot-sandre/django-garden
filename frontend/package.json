{
    "name": "frontend",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite",
        "net": "vite --host --https",
        "build": "run-s build::prepare build::build build::moveindex build:clean",
        "preview": "vite preview",
        "build:prepare": "del ../main/webapp_statics/frontend/assets/* ../main/templates/index.html --force",
        "build:build": "vite build",
        "build:moveindex": "move-file ../main/webapp_statics/frontend/index.html ../main/templates/index.html",
        "build:clean": "del ./dist"
    },
    "dependencies": {
        "@snowind/header": "^0.0.8",
        "@snowind/state": "^0.0.2",
        "primeicons": "^6.0.1",
        "primevue": "^3.17.0",
        "restmix": "^0.0.6",
        "vue": "^3.2.31",
        "vue-router": "4"
    },
    "devDependencies": {
        "@iconify/json": "^2.1.26",
        "@snowind/plugin": "0.4.0",
        "@tailwindcss/forms": "^0.5.0",
        "@vitejs/plugin-vue": "^2.3.1",
        "@vue/compiler-sfc": "^3.2.31",
        "autoprefixer": "^10.4.4",
        "del-cli": "^5.0.0",
        "move-file-cli": "^3.0.0",
        "npm-run-all": "^4.1.5",
        "path": "^0.12.7",
        "postcss": "^8.4.12",
        "sass": "^1.50.0",
        "tailwindcss": "^3.0.23",
        "tailwindcss-semantic-colors": "^0.2.0",
        "tslib": "^2.3.1",
        "typescript": "^4.6.3",
        "unplugin-icons": "^0.15.1",
        "unplugin-vue-components": "^0.22.12",
        "vite": "^4.0.4"
    }
}